{
  "name": "component-library",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run write:env -s && ng build",
    "start": "npm run write:env -s && ng serve --proxy-config proxy.conf.js",
    "lint": "ng lint && stylelint \"src/**/*.scss\"",
    "test": "npm run write:env -s && ng test",
    "test:ci": "npm run write:env -s && npm run lint -s && ng run component-library:test:ci",
    "e2e": "npm run write:env -s && ng e2e",
    "docs": "hads ./docs -o",
    "write:env": "ngx-scripts env npm_package_version",
    "generate": "ng generate",
    "build-component": "cd component-lib && ng build ircc-ds-angular-component-library",
    "build-component-watch": "cd component-lib && ng build ircc-ds-angular-component-library --watch",
    "prettier": "prettier --write \"./**/*.{js,ts,json,html,css}\""
  },
  "dependencies": {
    "@angular/animations": "~15.2.10",
    "@angular/cdk": "^14.2.7",
    "@angular/cdk-experimental": "^14.2.7",
    "@angular/common": "~15.2.10",
    "@angular/compiler": "~15.2.10",
    "@angular/core": "~15.2.10",
    "@angular/forms": "~15.2.10",
    "@angular/localize": "~15.2.10",
    "@angular/platform-browser": "~15.2.10",
    "@angular/platform-browser-dynamic": "~15.2.10",
    "@angular/router": "~15.2.10",
    "@fontsource/inter": "^4.5.14",
    "@fontsource/lato": "^4.5.10",
    "@fortawesome/fontawesome-free": "^6.1.2",
    "@gilsdav/ngx-translate-router": "^5.1.1",
    "@gilsdav/ngx-translate-router-http-loader": "^2.0.0",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@popperjs/core": "^2.11.0",
    "bootstrap": "^5.0.2",
    "ircc-ds-angular-component-library": "file:projects/ircc-ds-angular-component-library",
    "@ircc-ca/ds-sdc-core": "file:../core-library",
    "ngx-highlightjs": "^7.0.1",
    "ngx-markdown": "^14.0.1",
    "rxjs": "^7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^15.0.0",
    "@angular-devkit/build-angular": "~15.2.11",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.11",
    "@angular/compiler-cli": "~15.2.10",
    "@angular/language-service": "~15.2.10",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngneat/until-destroy": "^9.0.0",
    "@ngx-rocket/scripts": "^5.2.2",
    "@types/jest": "^28.1.8",
    "@types/node": "^14.18.26",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-prettier": "^4.2.1",
    "hads": "^3.0.0",
    "https-proxy-agent": "^5.0.0",
    "jest": "^28.1.3",
    "ng-packagr": "^15.2.2",
    "postcss": "^8.4.5",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "stylelint": "~14.11.0",
    "stylelint-config-recommended-scss": "~7.0.0",
    "stylelint-config-standard": "~28.0.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.1.0",
    "typescript": "~4.9.5"
  }
}
